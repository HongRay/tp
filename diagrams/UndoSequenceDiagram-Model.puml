@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":CommandHistory" as CommandHistory MODEL_COLOR
end box

[-> Model : undo()
activate Model

Model -> CommandHistory : setAddressBook(ReadOnlyAddressBook)
activate CommandHistory

CommandHistory -> CommandHistory :resetData(ReadOnlyAddressBook)
CommandHistory --> Model :
deactivate CommandHistory

[<-- Model
deactivate Model

@enduml
